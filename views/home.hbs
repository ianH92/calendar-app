<!DOCTYPE html>
<html lang="en-US">
	<head>
		<meta charset="UTF-8">
		<title>Calendar-app Home Page</title>
		<link href="/stylesheets/calendarStyle.css" type="text/css" rel="stylesheet">
		<link href="/stylesheets/headerStyle.css" type="text/css" rel="stylesheet">
	</head>

	<body>
		<a href="/" rel="homepage" title="homepage"><h1>Event Calendar</h1></a>
		<div id="links">
			<a href="/home">HOME PAGE</a>
			<a href="/about">ABOUT</a>
			<a href="/events/createEvent">CREATE EVENT</a>
			<a href="/todo/createTodo">CREATE TODO</a>
			<a href="/logout/">LOGOUT</a>
		</div>
		<div id="page">
			<div id="calendar">
				<h3>Calendar</h3>
				<table>
					<tr>
						<td id="prev" class="top"><span id="prevMonth"></span></td>
						<td id="curr" colspan="5" class="top"><strong><span id="currMonth"></span></strong></td>
						<td id="next" class="top"><span id="nextMonth"></span></td>
					</tr>
					<tr>
						<th>Sun</th>
						<th>Mon</th>
						<th>Tue</th>
						<th>Wed</th>
						<th>Thu</th>
						<th>Fri</th>
						<th>Sat</th>
					</tr>
					<tr>
						<td id="1" class="empty notactive"></td>
						<td id="2" class="empty notactive"></td>
						<td id="3" class="empty notactive"></td>
						<td id="4" class="empty notactive"></td>
						<td id="5" class="empty notactive"></td>
						<td id="6" class="empty notactive"></td>
						<td id="7" class="empty notactive"></td>
					</tr>
					<tr>
						<td id="8" class="empty notactive"></td>
						<td id="9" class="empty notactive"></td>
						<td id="10" class="empty notactive"></td>
						<td id="11" class="empty notactive"></td>
						<td id="12" class="empty notactive"></td>
						<td id="13" class="empty notactive"></td>
						<td id="14" class="empty notactive"></td>
					</tr>
					<tr>
						<td id="15" class="empty notactive"></td>
						<td id="16" class="empty notactive"></td>
						<td id="17" class="empty notactive"></td>
						<td id="18" class="empty notactive"></td>
						<td id="19" class="empty notactive"></td>
						<td id="20" class="empty notactive"></td>
						<td id="21" class="empty notactive"></td>
					</tr>
					<tr>
						<td id="22" class="empty notactive"></td>
						<td id="23" class="empty notactive"></td>
						<td id="24" class="empty notactive"></td>
						<td id="25" class="empty notactive"></td>
						<td id="26" class="empty notactive"></td>
						<td id="27" class="empty notactive"></td>
						<td id="28" class="empty notactive"></td>
					</tr>
					<tr>
						<td id="29" class="empty notactive"></td>
						<td id="30" class="empty notactive"></td>
						<td id="31" class="empty notactive"></td>
						<td id="32" class="empty notactive"></td>
						<td id="33" class="empty notactive"></td>
						<td id="34" class="empty notactive"></td>
						<td id="35" class="empty notactive"></td>
					</tr>
					<tr>
						<td id="36" class="empty notactive"></td>
						<td id="37" class="empty notactive"></td>
						<td id="38" class="empty notactive"></td>
						<td id="39" class="empty notactive"></td>
						<td id="40" class="empty notactive"></td>
						<td id="41" class="empty notactive"></td>
						<td id="42" class="empty notactive"></td>
					</tr>
				</table>
			</div>
			<div id="todos">
				<h3 id="todoh3">To Do List</h3>
				{{#each todos}}
				<div id={{this._id}} class="todo">
					<p>{{this.name}}</p>
					<p>Priority {{this.priority}}</p>
				</div>
				{{/each}}
			</div>
		</div>
		<script src="/javascripts/calendarGenerate.js"></script>
	</body>
	
</html>